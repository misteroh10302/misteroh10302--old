/* Responsive tables for IE9+ */
html:not(.lt-ie9) {
  .table--responsive {
    @include at-query($max, $small) {
      thead {
        display: none;
      }

      tr {
        display: block;
      }

      tbody{
        border-top: 1px solid $colorBorder;
      }

      tbody td:not(:first-child){
        border-top: 1px solid rgba($colorBorder, 0.2);
      }

      tfoot{
        border-top: 1px solid $colorBorder;
        border-bottom: 2px solid $colorBorder;
      }

      // IE9 table layout fixes
      tr,
      td {
        float: left;
        clear: both;
        width: 100%;
        border-bottom: none;
      }

      th,
      td {
        display: block;
        text-align: right;
        padding: 10px;
      }

      td:before {
        content: attr(data-label);
        float: left;
        @include uppercase;
        font-size: 12px;
        padding-right: 10px;
      }
    }
  }

  @include at-query($max, $small) {
    .table--small-hide {
      display: none !important;
    }

    .table__section + .table__section {
      position: relative;
      margin-top: 10px;
      padding-top: 15px;

      &:after {
        content: '';
        display: block;
        position: absolute;
        top: 0;
        left: 15px;
        right: 15px;
        border-bottom: 1px solid $colorBorder;
      }
    }
  }
}


