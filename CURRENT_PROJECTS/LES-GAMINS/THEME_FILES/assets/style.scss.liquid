// Style the dropdown
.nice-select {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  background-color: #fff;
  border-radius: 0;
  border: 1px solid $pblue;;
  box-sizing: border-box;
  clear: both;
  cursor: pointer;
  display: block;
  float: left;
  font-family: inherit;
  font-size: $font_size;
  font-weight: normal;
  height: 37px;
  line-height: 35px;
  outline: none;
  padding-left: $dropdown_padding;
  padding-right: $dropdown_padding + 12;
  position: relative;
  text-align: center;
  transition: all 0.2s ease-in-out;
  user-select: none;
  white-space: nowrap;
  width: 100%;

  &:hover {
    border-color: $pblue;
  }
  &:active, &:focus {
    border-color: $pblue;
  }
  // Arrow


  .current:after {
    border-bottom: 2px solid white;
    border-right: 2px solid white;
    background-image: url(https://cdn.shopify.com/s/files/1/1381/5403/files/Dropdown_Arrows-01.svg?10333985377927601513);
    content:  ' ';
    display: inline-block;
    height: 5px;
    margin-top: -4px;
    pointer-events: none;
    right: 12px;
    top: 50%;
    //transform-origin: 66% 66%;
    //transform: rotate(45deg);
    //transition: all 0.15s ease-in-out;
    width: 5px;
    width: 5px;
    margin-left: 5px;
        content: ' ';
    display: inline-block;
    height: 20px;
    margin-top: -4px;
    pointer-events: none;
    right: 12px;
    top: 11px;
    width: 20px;
    /* width: 105px; */
    /* margin-left: 5px; */
    background-size: cover;

  }
    .black::before {
    content: '';
     display: none;
     width: 9px;
     height: 9px;
     -moz-border-radius: 15px;
     -webkit-border-radius: 15px;
     border-radius: 50%;
      margin-right: 4px;
     background-color: black;
            left: 6px;
    position: absolute;
    top: 14.2px;


  }

  .blue, .pink, .red, .yellow, .multi, .grey,.gray, .white, .green{
     @extend .black;


  }

  .blue::before {
     background-color:blue;
  }



  .pink::before {
     background-color:pink;
  }

  .red::before {
     background-color:red;
  }

  .yellow::before {
     background-color:yellow;
  }

  .multi::before {
         background-image: url(https://cdn.shopify.com/s/files/1/1381/5403/files/Screen_Shot_2017-02-06_at_4.25.10_PM.png?7732940642794810012);
    background-size: 20px;
    border: 1px solid #e6e6e6;
  }

  .grey::before {
     background-color:gray;
  }

    .gray::before {
     background-color:gray;
  }

  .white::before {
     background-color:white;
     border: 1px solid lightgray;
  }

  .green::before {
     background-color:green;
  }

  &.open {
    @extend :active;
    &:after {
      transform: rotate(-135deg);
    }
    .list {
      opacity: 1;
      pointer-events: auto;

    }
  }
  &.disabled {
    border-color: lighten($gray_light, 2%);
    color: $gray;
    pointer-events: none;
    &:after {
      border-color: lighten($arrow_color, 20%);
    }
  }

  // Modifiers
  &.wide {
    width: 100%;
    .list {
      left: 0 !important;
      right: 0 !important;
    }
  }
  &.right {
    float: right;
    .list {
      left: auto;
      right: 0;
    }
  }
  &.small {
    font-size: $font_size_small;
    height: $input_height_small;
    line-height: $input_height_small - 2;
    &:after {
      height: 4px;
      width: 4px;
    }
    .option {
      line-height: $input_height_small - 2;
      min-height: $input_height_small - 2;
    }
  }

  // List and options
  .list {
    background-color: #fff;
    border-radius: 0;
    box-shadow: 0 ;
    box-sizing: border-box;
    margin-top: 0px;
    opacity: 0;
    overflow: hidden;
    padding: 0;
    pointer-events: none;
    position: absolute;
    top: 100%;
    left: -2px;
    transform-origin: 50% 0;
    transition: all .2s cubic-bezier(0.5, 0, 0, 1.25), opacity .15s ease-out;
    z-index: 9;
    width: 102%;
    border:1px solid $pblue;
    border-top:0px;
    text-transform: uppercase;
    font-size: .8em;
    letter-spacing: 2px;
    &:hover .option:not(:hover) {
      background-color: transparent !important;
    }
  }

  .current {
     text-transform: uppercase;
    font-size: .9em;
    letter-spacing: 2px;
  }
  .option {
    cursor: pointer;
    font-weight: 400;
    line-height: $input_height - 2;
    list-style: none;
    min-height: $input_height - 2;
    outline: none;
    padding-left: $dropdown_padding;
    padding-right: $dropdown_padding + 11;
    text-align: center;
    transition: all 0.2s;
    &:hover, &.focus, &.selected.focus {
      background-color: transparent;
    }
    &.selected {
      font-weight: bold;
    }
    &.disabled {
      background-color: transparent;
      color: $gray;
      cursor: default;
    }
  }
}

// Use display instead of opacity for IE <= 10
.no-csspointerevents .nice-select {
  .list {
    display: none;
  }
  &.open {
    .list {
      display: block;
    }
  }
