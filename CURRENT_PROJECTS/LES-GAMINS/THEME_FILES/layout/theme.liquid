<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->
<head>

  <!-- ========================== Pipeline Theme ============================ -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  {% if settings.favicon_enable %}
  <link rel="icon" href="https://cdn.shopify.com/s/files/1/1381/5403/files/LesGamins_Favicon_Options-04.png?17784438347045976595" type="image/png" />
  {% endif %}

  <!-- Title and description ================================================ -->
  <title>
  {{ page_title }}{% if current_tags %}{% assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>

  {% if page_description %}
  <meta name="description" content="{{ page_description | escape }}">
  {% endif %}

  <!-- Product meta ========================================================= -->
  {% include 'social-meta-tags' %}

  <!-- Helpers ============================================================== -->
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="{{ settings.color_primary }}">

  {% unless settings.cart_type == 'page' %}
  <!-- Ajaxify Cart Plugin ================================================== -->
  {{ 'ajaxify.scss.css' | asset_url | stylesheet_tag }}
  {% endunless %}

  <!-- CSS ================================================================== -->
  {{ 'style.scss.css' | asset_url | stylesheet_tag }}

  <!-- Header hook for plugins ============================================== -->
  {{ content_for_header }}

  {% include 'oldIE-js' %}

  {{ '//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js' | script_tag }}
  {{ 'modernizr.min.js' | asset_url | script_tag }}
  {{ 'api.jquery.js' | shopify_asset_url | script_tag }}

  <!-- Slick Slider Plugin ================================================= -->
  {{ 'slick.min.js' | asset_url | script_tag }}
  {{ 'parallax.min.js' | asset_url | script_tag }}


  <!-- Instagram Plugin ==================================================== -->
  {% if template == 'index' and settings.instagram_id != blank%}
    {{ 'instafeed.js' | asset_url | script_tag }}
  {% endif %}

  {% comment %}
    If you store has customer accounts disabled, you can remove the following JS file
  {% endcomment %}
  {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
  {% endif %}

  {{'jquery.nice-select.min.js' | asset_url | script_tag}}
  {% include 'fonts' %}

  <!-- Options Select Plugin ==================================================== -->
   {{ 'option_selection.js' | shopify_asset_url | script_tag }}

  <!-- Cookie  Plugin ==================================================== -->
  {{'cookie.js' | asset_url | script_tag}}

</head>

<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >

  <div class="header__wrapper">
<!--
    {% if settings.header_enable_account or settings.header_enable_search or settings.header_message or settings.disable != '' %} -->
  {% if settings.disable %}

        <div class="info-bar showMobile">
            <div class="wrapper text-center">
              {% if settings.header_message != '' %}
              <div class="desktop header-message uppercase{% if settings.header_enable_account or settings.header_enable_search %} header-message--crowded{% endif %}">
                {{ settings.header_message }}
               </div>

               <div class="mobile header-message uppercase{% if settings.header_enable_account or settings.header_enable_search %} header-message--crowded{% endif %}">
                  {{ settings.header_message_mobile }}
               </div>


    {% endif %}

        {% if settings.header_enable_search %}
          <div class="header-search__wrapper">
            <div class="header-search">
              <form action="/search" method="get" class="input-group search" role="search">
                {% if settings.search_only_products %}<input type="hidden" name="type" value="product">{% endif %}
                <input class="search-bar" type="search" name="q">
                <button type="submit" class="btn search-btn"></button>
              </form>
            </div>
          </div>
        {% endif %}

      </div>
    </div>
    {% endif %}

    <header class="site-header" role="banner">
      <div class="wrapper">
        <div class="nav--desktop">
          {% include 'nav-contents-full' %}
        </div>
        <div class="nav--mobile">
          {% include 'nav-contents' %}
        </div>
      </div>
    </header>
  </div>

  <main class="main-content" role="main">


    {{ content_for_layout }}
  </main>

  <footer class="site-footer-wrapper" role="contentinfo">

    <div class="wrapper site-footer">
      <div class="grid-uniform">

        <div id="footer-logo" class="col-foot-large">

          <a href="/" itemprop="url" class="footer-logo" id="footerLogo" style="opacity:1;">
            <img src="https://cdn.shopify.com/s/files/1/1381/5403/files/Circular_G_Logo_Outlined-01.svg?10772076332664223768" alt="{{ shop.name }}" itemprop="logo" id="footerLogoImg"/>
          </a>


        <!--   {% elsif settings.footer_logo_enable == 'custom' %}
          <a href="/" itemprop="url" class="footer-logo" id="footerLogo" style="opacity:0;">
            <img src="https://cdn.shopify.com/s/files/1/1381/5403/files/Circular_G_Logo_Outlined-01.svg?10772076332664223768" alt="{{ shop.name }}" itemprop="logo" id="footerLogoImg"/>
          </a>
          {% endif %} -->
             <h6><span id="lesgaminsfooter">Les Gamins</span> &#149; Made For Play. <br> &copy; Les Gamins Inc.</h6>

        </div>
        <div class="col-foot-small">
          <a href="/pages/about-us">About</a>

        </div>
        <div class="col-foot-small">
          <a href="/pages/contact">Contact</a>
          <a href="/pages/contact/#contact-us-footer">Customer Service</a>
          <a href="/pages/faqs">FAQ</a>

        </div>
        <div class="col-foot-small">
          <a href="/pages/privacy-policy">Privacy Policy</a>
        </div>
        <div class="col-foot-small shipping-return">
            <a id="returns-footer" href="/pages/returns-and-shipping#returns">Returns </a>
          <a href="/pages/returns-and-shipping#shipping">Shipping</a>

        </div>
        <div class="col-foot-small">
          <a href="/pages/terms-and-conditions">Terms </a>
        </div>
        <div class="col-foot-small">
          <a href="/blogs/news">Blog</a>
        </div>
        <div id="footer-news" class="col-foot-large" style="float:right;">
          {% if settings.footer_newsletter_enable %}
            <div class="grid__item {{ footer_column_width }}" style="display:none;">
              <p>{{ settings.footer_newsletter_text }}</p>
              {% include 'newsletter-form' %}
            </div>
          {% endif %}

          <!-- Begin MailChimp Signup Form -->
              <div id="mc_embed_signup">
              <form method="post" id="chimp">
                    <p>SIGN UP AND RECEIVE 15% OFF YOUR FIRST ORDER!</p>
                    <em style="display:none;">Thank you for signing up!</em>
                    <input type="email" id="mail-name" placeholder="your-email@example.com" style="height: auto;padding: 6px;font-size: 9px; width:60%;float: left;" name="EMAIL" class="required email" id="EMAIL">
                    <input type="submit"   style="background-color: #222944;    font-size: 7.5px;
    padding: 7.25px 3px;width: 40%;letter-spacing: 2px; float:left;text-transform: uppercase;letter-spacing: 4px;text-indent: 4px;" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="button">

                  </form>
              </div>

              <!--End mc_embed_signup-->

          <div class="social-icons-footer clear">
            <ul>
              <a target="_blank" href="https://www.instagram.com/lesgamins_nyc">
                <img id="insta" src="https://cdn.shopify.com/s/files/1/1381/5403/files/FooterSocialMediaIcons_Instagram-new.svg?9087594228766712187" alt="Instagram">
              </a>
              <a target="_blank" href="https://www.facebook.com/lesgaminsnyc/">
                <img id="face"src="https://cdn.shopify.com/s/files/1/1381/5403/files/FooterSocialMediaIconsFacebook-new.svg?13161135896568736191" alt="Facebook">
              </a>
              <a target="_blank" href="mailto:hello@les-gamins.com">
                <img src="https://cdn.shopify.com/s/files/1/1381/5403/files/FooterSocialMediaIconsEnvelope-new.svg?9087594228766712187" alt="Email">
              </a>
            </ul>
          </div>

        </div>
    </div>
  </footer>

  <div class="error-pop">
    <p class="close-error">x</p>
      <h1>Oops!</h1>
    <class class="error"></class>
  </div>

{% include 'collection-filter-v2' %}
  {% include "bagtots" %}
  {% include "email-capture" %}
  {% include "refferral-program" %}
  <!-- {% include 'linked-options' %} -->
  {{ 'fastclick.min.js' | asset_url | script_tag }}
  {{ 'shop.js' | asset_url | script_tag }}

{% unless customer %}
   <script>
   function explode(){
                  $('#email-capture').show(200).fadeIn(200);
                  $('.ajaxcart__overlay').css({'visibility':'visible','opacity':'0.7'});

              }
              function closeMe(){
                $('#email-capture').hide(200).fadeOut(200);
                 $('.ajaxcart__overlay').css({'visibility':'hidden','opacity':'0.7'});

               }

      function set_cookie() {
          var secs = 86400;
          var now = new Date();
          var exp = new Date(now.getTime() + secs*1000);
          var status = '';
          document.cookie = 'ExpirationCookieTest=1; expires='+exp.toUTCString();
      }

    $(document).on('ready',function(){

       if (document.cookie && document.cookie.indexOf('ExpirationCookieTest=1') != -1) {
              // console.log('Cookie is present');
          } else {
              // console.log('Cookie is NOT present. It may be expired, or never set');
               set_cookie();
               explode();
          }
      });


    </script>
{% endunless %}

  <script>
    {% if newHash %}
      $(function() {
        timber.updateHash('{{ newHash }}');
      });
    {% endif %}
    {% if resetPassword %}
      $(function() {
        timber.resetPasswordSuccess();
      });
    {% endif %}
  </script>

  {% comment %}
    Modal Cart Documentation:
    - http://shopify.com/timber#ajax-cart
  {% endcomment %}
  {% unless settings.cart_type == 'page' %}
    {{ 'handlebars.min.js' | asset_url | script_tag }}
    {% include 'ajax-cart-template' %}
    {{ 'ajaxify.js' | asset_url | script_tag }}
    <script>
      jQuery(document).ready(function(){
        var oldBrowser = $( "html" ).hasClass( "lt-ie9" );
        if(!oldBrowser) {
         ajaxifyShopify.init({
           method: 'modal',
           wrapperClass: 'wrapper',
           formSelector: '#AddToCartForm',
           addToCartSelector: '#AddToCart',
           cartCountSelector: '.cartCount',
           cartCostSelector: '.cartCost',
           toggleCartButton: '.CartToggle',
           useCartTemplate: true,
           btnClass: 'btn',
           moneyFormat: {{ shop.money_format | json }},
           disableAjaxCart: false,
           enableQtySelectors: true,
           prependDrawerTo: 'body',
           onToggleCallback: function(){
             jQuery('body').trigger('ajaxCart.afterCartLoad')
           }
         });
        }
      });

    </script>
  {{ 'main-script.js' | asset_url | script_tag }}
  {% endunless %}
</body>
</html>
